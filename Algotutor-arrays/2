class Solution {
    public int numIdenticalPairs(int[] nums) {
        int flag=0;
        int size=nums.length;
        for(int i=0;i<size;i++)
        {
            for(int j=i+1;j<size;j++)
            {
                if(nums[i]==nums[j])
                flag++;
            }
        }
        return flag;
    }
}